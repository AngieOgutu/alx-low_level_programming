#include <stdio.h>
#include "main.h"

/**
 * _isalpha - checks if it is an alphabetic character and returns 1
 * @c - input to be checked
 * Return: 1 if alphabet, 0 if otherwise
 */

int _isalpha(int c)
{
	if ((c > 64 && c < 91) || (c > 96 && c < 123))
	{
		return (1);
	}

	else
	{
		return (0);
	}
}
